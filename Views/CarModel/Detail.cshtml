@using TurboazTask1.Models;
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    ViewData["Title"] = "Detail";
}

@model List<Car> 

<h1>Model Detail</h1>

@foreach (Car car in Model)
{
        <ul>

            <li>
                <a asp-controller="Car" asp-action="Detail" asp-route-id="@car.CarId">@car.Name</a>
            </li>
        </ul>
}


