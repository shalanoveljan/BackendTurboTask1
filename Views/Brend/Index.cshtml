@using TurboazTask1.Models;
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@{
    ViewData["Title"] = "Index";
}

@model List<Brend>

<h1>Brend Page</h1>

@foreach (Brend brend in Model)
{
    <ul>
        <li>
            <a asp-controller="Brend" asp-action="Detail" asp-route-id="@brend.BrandId">@brend.BrandName</a>
        </li>
    </ul>
}

